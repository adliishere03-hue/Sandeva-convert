{
  "version": 2,
  "builds": [
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "upload.js", "use": "@vercel/node" },
    { "src": "*.css", "use": "@vercel/static" },
    { "src": "*.js", "use": "@vercel/static" },
    { "src": "*.mp4", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/upload", "dest": "/upload.js" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
